{
  "manifest_version": 3,
  "name": "Check JS Functions",
  "version": "1.0",
  "description": "Check if the current page contains alert, confirm, or prompt functions.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}